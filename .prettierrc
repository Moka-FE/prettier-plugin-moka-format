{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "plugins": ["./lib/src/index.js"],
  "configuredRules": ["importSort"],
  "parserPlugins": ["decorators-legacy","jsx","typescript"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importAliasRegExpList": ["^@/","^(@common)"]
}
