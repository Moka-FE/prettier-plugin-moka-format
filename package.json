{
  "name": "prettier-plugin-moka-format",
  "version": "1.1.1",
  "description": "prettier plugin for moka format",
  "main": "lib/src/index.js",
  "repository": {
    "url": "https://github.com/pipi-1997/prettier-plugin-moka-format",
    "type": "git"
  },
  "homepage": "https://github.com/pipi-1997/prettier-plugin-moka-format",
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "preexample": "npm run compile",
    "test": "jest",
    "example": "prettier --config ./examples/.prettierrc --plugin lib/src/index.js",
    "prepublishOnly": "npm run compile && npm run test",
    "postversion": "git push && git push --tags",
    "lint:fix": "eslint \"src/**/*.ts\" --fix &&  prettier --write \"src/**/*.ts\" ",
    "commit": "git-cz",
    "version": "npm run lint:fix && git add -A src",
    "release": "standard-version"
  },
  "keywords": [
    "prettier",
    "plugin",
    "sort",
    "import",
    "typescript",
    "javascript"
  ],
  "author": {
    "name": "Zhuang Quan",
    "email": "zhuangpipi01@gmail.com",
    "url": "https://github.com/pipi-1997"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.13.10",
    "@babel/generator": "7.13.9",
    "@babel/parser": "7.14.6",
    "@babel/traverse": "7.13.0",
    "@babel/types": "7.13.0",
    "eslint-config-prettier": "^8.3.0",
    "find-up": "^5.0.0",
    "javascript-natural-sort": "^0.7.1",
    "lodash": "4.17.21",
    "micromatch": "^4.0.4",
    "postcss-sorting": "^7.0.1",
    "recast-yx": "^0.18.12",
    "slash": "^3.0.0",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/chai": "4.2.15",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/micromatch": "^4.0.2",
    "@types/node": "14.14.34",
    "@types/prettier": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "26.6.3",
    "lint-staged": "^12.1.2",
    "prettier": "2.3.1",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  },
  "peerDependencies": {
    "prettier": "2.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint:fix",
    "*.{js,ts,css,md}": "npm run lint:fix"
  }
}
